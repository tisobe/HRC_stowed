    <script language="JavaScript">
        function WindowOpener(imgname) {
            msgWindow = open("","displayname","toolbar=no,directories=no,menubar=no,location=no,scrollbars=no,status=no,,width=850,height=850,resize=yes");
            msgWindow.resizeTo(850,850);
            msgWindow.document.clear();
            msgWindow.document.write("<html><title>Trend plot:   "+imgname+"</TITLE>");
            msgWindow.document.write("<body bgcolor='black'>");
            msgWindow.document.write("<img src='../Maps/Hrc_s_125_hi/"+imgname+"' border =0 width=100% ><p></p></body></html>")
            msgWindow.document.close();
            msgWindow.focus();
        }


        function WindowOpener2(imgname) {
            msgWindow = open("","displayname","toolbar=no,directories=no,menubar=no,location=no,scrollbars=yes,status=no,,width=250,height=980,resize=yes");
            msgWindow.resizeTo(250,980);
            msgWindow.document.clear();
            msgWindow.document.write("<html><title>Trend plot:   "+imgname+"</TITLE>");
            msgWindow.document.write("<body bgcolor='black'>");
            msgWindow.document.write("<p style='text-align:center'><img src='../Maps/Hrc_s_125_hi/"+imgname+"' border =0 width=85%  ></p></body></html>")
            msgWindow.document.close();
            msgWindow.focus();
        }
    </script>

